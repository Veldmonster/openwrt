	;;
zte-q7)
	set_wifi_led "$board:blue:status"
	;;
youku-yk1)
	ucidef_set_led_default "power" "power" "$board:blue:power" "1"
	set_wifi_led "$board:blue:air"
	set_usb_led "$board:blue:usb"
	;;
>>>>>>> 871372c42a3fc9c4b33f5c6011742d610a2e5600
esac

board_config_flush

exit 0
