#!/bin/sh

. /lib/functions/uci-defaults.sh
. /lib/ramips.sh

set_usb_led() {
	ucidef_set_led_usbdev "usb" "USB" "$1" "1-1"
}

set_wifi_led() {
	ucidef_set_led_netdev "wifi_led" "wifi" "$1" "wlan0"
}


board=$(ramips_board_name)

board_config_update

